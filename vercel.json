{
    "version": 2,
    "builds": [
      {
        "src": "api/main.go",
        "use": "@vercel/go"
      }
    ],
    "routes": [
      {
        "src": "/main",
        "dest": "/api/main"
      },
      {
        "src": "/",
        "dest": "/public/form.html"
      },
      {
        "src": "/map",
        "dest": "/public/map.html"
      }
    ]
}
  
